{
	"function": {
		"api": {
			"build": true,
			"providerPlugin": "awscloudformation",
			"service": "Lambda"
		}
	},
	"api": {
		"samscha": {
			"service": "API Gateway",
			"providerPlugin": "awscloudformation",
			"dependsOn": [
				{
					"category": "function",
					"resourceName": "api",
					"attributes": [
						"Name",
						"Arn"
					]
				}
			]
		},
		"graphql": {
			"service": "AppSync",
			"providerPlugin": "awscloudformation",
			"output": {
				"authConfig": {
					"additionalAuthenticationProviders": [],
					"defaultAuthentication": {
						"authenticationType": "API_KEY",
						"apiKeyConfig": {
							"description": "",
							"apiKeyExpirationDays": 7
						}
					}
				}
			}
		}
	}
}