{
    "name": "samscha",
    "version": "1.0.0",
    "description": "Personal website for Samuel Cha",
    "repository": "https://github.com/samscha/samscha.git",
    "author": "Samuel Cha <sunchipssam@gmail.com>",
    "license": "MIT",
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "^6.1.1",
        "@fortawesome/free-brands-svg-icons": "^6.0.0",
        "@fortawesome/free-regular-svg-icons": "^6.0.0",
        "@fortawesome/free-solid-svg-icons": "^6.0.0",
        "@fortawesome/react-fontawesome": "^0.2.0",
        "prop-types": "^15.7.2",
        "react": "^18.0.0",
        "react-dom": "^18.0.0",
        "react-router-dom": "^6.2.1",
        "react-scripts": "^5.0.0",
        "sass": "^1.32.8"
    },
    "devDependencies": {
        "@babel/core": "^7.13.8",
        "@babel/plugin-proposal-class-properties": "^7.13.0",
        "@babel/preset-env": "^7.13.8",
        "@babel/preset-react": "^7.12.10",
        "@types/jest": "^29.1.2",
        "prettier": "2.8.3",
        "react-test-renderer": "^18.0.0"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "eject": "react-scripts eject",
        "test:watch": "jest --watch",
        "test": "jest --no-cache",
        "pretti:all": "npm run pretti:api && npm run pretti:fe",
        "pretti:api": "prettier --write \"amplify/backend/function/api/src/**/*.js\"",
        "pretti:fe": "prettier --write \"src/**/*.{jsx,js}\"",
        "sync-public-repo": "git checkout develop && git fetch upstream && git merge upstream/develop && git push && git checkout main && git fetch upstream && git merge upstream/main && git push"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "jest": {
        "moduleNameMapper": {
            "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
            "\\.(css|scss)$": "<rootDir>/__mocks__/styleMock.js"
        },
        "setupFilesAfterEnv": [
            "<rootDir>src/setupTests.js"
        ]
    }
}
